{
  "name": "writenpost",
  "version": "1.0.0",
  "description": "This is write and post application",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "build-prod": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/ dist/",
    "build-dev": "rm -rf dist && webpack --config webpack/webpack.config.dev.js  && cp -a assets/ dist/",
    "prod": "serve ./dist -s",
    "lint": "eslint src",
    "ngrok": "ngrok http -region eu 8080",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote --trailing-comma all",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemadriDasari1990/pp-web.git"
  },
  "keywords": [
    "react",
    "redux",
    "async"
  ],
  "author": "Hemadri Dasari & Rajesh Pemmasani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hemadriDasari1990/pp-web/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.2",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.19.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "img-loader": "^3.0.1",
    "jest": "^21.1.0",
    "lint-staged": "^4.0.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preload-webpack-plugin": "^1.2.2",
    "prettier": "^1.5.2",
    "react-test-renderer": "^16.13.0",
    "sass-loader": "^7.3.1",
    "serve": "^6.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.18.0",
    "babel-plugin-emotion": "^10.0.29",
    "css-loader": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "emotion": "^7.3.1",
    "file-loader": "^3.0.1",
    "firebase": "^5.8.3",
    "history": "^4.10.1",
    "html-loader": "^0.5.5",
    "immutable": "^4.0.0-rc.12",
    "install": "^0.12.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.2",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "notistack": "^0.4.2",
    "offline-plugin": "^4.8.3",
    "react": "^16.13.0",
    "react-autosuggest": "^9.4.3",
    "react-code-splitting": "^1.1.1",
    "react-dom": "^16.13.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.1",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack/webpack.config.dev.js"
        }
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "no-confusing-arrow": 0,
      "no-shadow": 0,
      "no-underscore-dangle": 0,
      "semi": [
        1,
        "never"
      ],
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "import/no-duplicates": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  }
}
